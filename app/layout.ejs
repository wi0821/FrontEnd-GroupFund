    <%- contents %>

    <!-- 登入頁面 Modal -->
    <div class="modal fade" id="loginModal" tabindex="-1" aria-labelledby="loginModalLabel" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered modal-dialog-scrollabl">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title" id="LoginModalLabel">會員登入</h4>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">

            <!-- 會員登入 -->
            <form>
              <div class="row mb-3">
                <label for="LoginAccount" class="col-sm-2 col-form-label fs-5 fw-bold">帳戶</label>
                <div class="col-sm-10">
                  <input type="text" class="form-control" id="LoginAccount" required>
                </div>
              </div>
              <div class="row mb-3">
                <label for="LoginPwd" class="col-sm-2 col-form-label fs-5 fw-bold">密碼</label>
                <div class="col-sm-10">
                  <input type="password" class="form-control" id="LoginPwd" required>
                </div>
              </div>

              <div class="row mb-3">
                <div class="col-sm-10 offset-sm-2">
                  <div class="form-check">
                    <input class="form-check-input" type="checkbox" id="chkRemember">
                    <label class="form-check-label" for="chkRemember">
                      記住我
                    </label>
                  </div>
                </div>
              </div>

              <div class="row mb-3">
                <div class="col-sm-10 offset-sm-2">
                    <p class="status text-danger fw-bold"></p>
                </div>
              </div>
              <div class="d-flex justify-content-end mx-2 me-3">
                <a type="button" class="btn btn-warning px-5 py-2 me-2" href="register.html">註冊</a>
                <button type="submit" id="btnLogin" class="btn btn-success px-5 py-2 me-2">登入</button>
                <!-- <button type="submit" id="btnLogOut" class="btn btn-outline-danger px-5 py-2">登出</button> -->
              </div>
            </form>
            <!-- 會員登入 -->

          </div>
        </div>
      </div>
    </div>
